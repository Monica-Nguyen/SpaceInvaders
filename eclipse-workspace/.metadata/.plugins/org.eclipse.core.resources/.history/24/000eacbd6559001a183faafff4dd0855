
public class SpaceScore {
	private int score;
	private int end;
	
	public void Space(int start, int end)
	{
		this.score = start;
		this.end = end;
	}
	
	public String toString() 
	{
		return "(\"Game score =" + score + "end =\" + end)";
	}
	
	public int getCounter()
	{
		return score;
	}
	
	public void count(int increment)
	{
		if (score < end)
		{
			score += increment;
		}
		else
		{
			count(1);
		}
	}
}
