public class Prey extends Animal {
	private String defence;
	private int herdSize;
	
	public Prey(char type, int health, String defence) {
		super( type, health);
		this.defence = "stampede";
		
	}
	
	public Prey(Prey toCopy){
		super(toCopy);
		
	}
	
	public String getDefence() {
		return "";
	}
	public void setDefence(String aDefence) {
		this.defence = (aDefence.equals("huddle")) ? aDefence : (aDefence.equals("hide")) ? aDefence : "stampede";
	}
	
	public int getHerdSize() {
		return 0;
	}
	
	public void setHerdSize(int size) {
		
	}
	
	public double getRelativeHealth() {
		return 0.0;
	}
	
	public String toString() {
		return "";
	}
}