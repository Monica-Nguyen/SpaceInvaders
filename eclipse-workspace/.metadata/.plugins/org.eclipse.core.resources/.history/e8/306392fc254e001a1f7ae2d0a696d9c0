public class Point {
//instance variables 
	private int xcoord;
	private int ycoord;
	
//Constructors: a copy constructor and a constructor that takes initial values of x/y coord.
	Point(int ycoord, int xcoord)
	{
		this.xcoord = xcoord;
		this.ycoord = ycoord; 
	}
//copy constructor
	Point(Point p)
	{
		this.xcoord = p.xcoord;
		this.ycoord = p.ycoord;
	}

//M: get method - returning value
	public int getXcoord()
	{
		return xcoord;
	}
//M: get method - returning value
	public int getYcoord()
	{
		return ycoord;
	}

//M: moving up, down, left, right for the object
	public int moveUp()
	{
		return ycoord;
		
	}
	public int moveDown()
	{
		return -ycoord;
		
	}
	public int moveRight()
	{
		return xcoord;
		
	}
	public int moveLeft()
	{
		return -xcoord;
	}
	
//M: distance - arguments (og point, point2) - returns double |og point - point2| 
	public int distance(int Point, int newPoint)
	{
		return newPoint;
	}
	
}
