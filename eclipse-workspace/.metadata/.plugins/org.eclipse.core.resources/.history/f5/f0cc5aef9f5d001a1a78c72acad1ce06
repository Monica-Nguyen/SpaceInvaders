public class Theatre {
	
	private String title;
	private int rating;
	
	public Theatre(String aTitle, int aRating)
	{
		this.title = aTitle; 
		this.rating = aRating; 
	}

	public Theatre(Theatre toCopy)
	{
	}
	
	public String setTitle(String title)
	{
		return this.title = title.toUpperCase();
	}

	public String getTitle()
	{
		return title;
	}
	public String getCategory()
	{
		String category; 
		if (rating >= 9) {
			category = "A";
		}
		else if (rating >= 7) {
			category = "B";
		}
		else if (rating >= 5) {
			category = "C";
		}
		else if (rating >= 3) {
			category = "D";
		}
		else category = "F";
		return category;
	}
	public int getRating()
	{
		return rating;
	}
	
	public int setRating (int aRating)
	{
		return rating;
	}
	

}
