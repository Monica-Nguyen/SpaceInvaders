

public class Ship extends MovableObject{
	

	public Ship(int xP, int yP, int e) {
		super(xP, yP, e, 3); 
	}
	

	public static void relocate(double x, double y) {
		Ship.relocate(x,y);
		
	}
	
	private void setX(int xP) {
		this.xPos = xPos;
	}
	
	public int getX() {
		return 0;
	}
	
	//making the animation smoother
	private double velX = 0;
	private double velY = 0;
	
	public void tick()
	{
		x+= velX;
		y=+ velY;
	}
	
	public void setVelX(double velX)
	{
		this.velX = velX;
	}
	public void setVelY(double velY)
	{
		this.velY = velY;
	}
	
	
}
	
