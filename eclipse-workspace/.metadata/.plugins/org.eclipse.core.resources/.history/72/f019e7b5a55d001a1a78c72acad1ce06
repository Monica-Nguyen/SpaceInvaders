public class Theatre {
	
	private String title;
	private int rating;
	
	public Theatre(String aTitle, int aRating)
	{
		this.title = aTitle.toUpperCase(); 
		if (aRating >= 0 && aRating <= 10)
		this.rating = aRating; 
		else
			return;
	}

	public Theatre(Theatre toCopy)
	{
		title = toCopy.title;
		rating = toCopy.rating;
	}
//Set title
	public String setTitle(String aTitle)
	{
		return this.title = aTitle.toUpperCase();
	}
//get title
	public String getTitle()
	{
		return title;
	}
//set rating so it is only in-between 0-10
	public void setRating (int aRating)
	{
		if (aRating >= 0 && aRating <= 10)
		this.rating = aRating;
	}
//get rating
	public int getRating()
	{
		return rating;
	}	
//category from A-F with a range of 10-0
		public String getCategory()
		{
			String category; 
			
			if (rating >= 9) 
			{
				category = "A";
			}
			else if (rating >= 7) 
			{
				category = "B";
			}
			else if (rating >= 5) 
			{
				category = "C";
			}
			else if (rating >= 3) 
			{
				category = "D";
			}
			else category = "F";
			
			return category;
		}
	
}
